    "import json\n",
    "\n",
    "url = \"http://localhost:8080/api/v1/movie\"\n",
    "\n",
    "f = open('movies.json')\n",
    "movies = json.load(f)\n",
    "\n",
    "for movie in movies:\n",
    "    movie['image'] = movie['images'][0]['path']\n",
    "    response = requests.request(\"POST\", url, data=movie)"
